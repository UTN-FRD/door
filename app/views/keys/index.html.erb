<h1>Lista de Llaves</h1>
 
<%= link_to 'Llave Nueva', new_key_path %>

  <table class="table table-striped">

    <tr>
      <th>Codigo</th>
      <th>Numero de copia</th>
      <th>  </th>
      <th colspan="3"></th>
    </tr>
   
    <% @key.each do |key| %>
      <tr>
        <td><%= key.codigo %></td>
        <td><%= key.numero_de_copia %></td>
        <td><%= link_to 'Editar', edit_key_path(key) %></td>
        <td><%= link_to 'Eliminar', key_path(key),
              method: :delete,
              data: { confirm: 'Â¿Estas seguro?' } %></td>
      </tr>
    <% end %>
  </table>