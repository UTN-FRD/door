 <%= form_for(@key) do |f| %>

     
      <% if @key.errors.any? %>
        <div id="error_explanation">
          <h2>
            <%= pluralize(@key.errors.count, "error") %> esta llave no puede ser guardada:
          </h2>
          <ul>
            <% @key.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
      <% end %>
     
      <div class="form-group">
        <%=f.label :codigo %>
        <%=f.text_field :codigo, :class => "form-control"%>
      </div>
     
     
     <div class="form-group">
        <%=f.label :numero_de_copia %>
        <%=f.text_field :numero_de_copia, :class => "form-control"%>
      </div>
     
     
      <div class="form-group"> 
        <%= link_to 'Volver', keys_path, class: 'btn btn-default' %>
          <%=f.submit 'Guardar', :class=> "btn btn-success"%>
      </div>


      
    <% end %>